{
  "name": "notes",
  "private": true,
  "description":
    "This is a presentation for the NSBE-SHPE presentation in Lehman College",
  "engines": {
    "node": "9.11.1",
    "npm": "5.6.x"
  },
  "scripts": {
    "start": "nodemon ./server.js",
    "db:migrate:make": "knex migrate:make",
    "db:migrate": "knex migrate:latest",
    "db:create": "node db/scripts/create.js",
    "db:seed": "knex seed:run",
    "db:destroy": "node db/scripts/destroy.js",
    "db:reset": "npm run db:destroy && npm run db:create && npm run db:migrate",
    "db:rollback": "knex migrate:rollback",
    "precommit": "pretty-quick --staged"
  },
  "dependencies": {
    "body-parser": "1.18.2",
    "connect-flash": "0.1.1",
    "cookie-parser": "1.4.3",
    "cookie-session": "2.0.0-beta.3",
    "dotenv": "5.0.1",
    "express": "4.16.3",
    "express-handlebars": "3.0.0",
    "knex": "0.14.5",
    "method-override": "2.3.10",
    "pg": "7.4.1",
    "ramda": "0.25.0"
  },
  "devDependencies": {
    "eslint": "4.19.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-config-standard": "11.0.0",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-node": "6.0.1",
    "eslint-plugin-promise": "3.7.0",
    "eslint-plugin-standard": "3.0.1",
    "husky": "0.14.3",
    "nodemon": "1.17.3",
    "prettier": "1.12.0",
    "pretty-quick": "1.4.1"
  }
}
